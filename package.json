  "description": "An app that makes decision making easier",
  "main": "server.js",
  "scripts": {
    "test": "export NODE_ENV=test&& jasmine",
    "start": "node src/server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrp && npm run build",
